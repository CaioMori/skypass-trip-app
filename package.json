{
  "name": "skypasstrip",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "prepare": "husky install",
    "lint": "eslint \"src/**/*.ts{x,}\" --fix",
    "commit": "yarn git-cz",
    "prettier": "yarn --silent prettier:format && git update-index --again",
    "prettier:check": "echo '>> Running Prettier checks...' && prettier --check '**/*[.ts|.js]*'",
    "prettier:format": "echo '>> Running Prettier fixes...' && prettier --write '**/*[.ts|.js]*' --loglevel warn",
    "ts:check": "tsc --noEmit",
    "web": "expo start --web"
  },
  "dependencies": {
    "expo": "~45.0.0",
    "expo-splash-screen": "~0.15.1",
    "expo-status-bar": "~1.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-web": "0.17.7",
    "@expo-google-fonts/roboto": "^0.2.2",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.7.0",
    "expo-font": "~10.1.0",
    "native-base": "^3.4.9",
    "phosphor-react-native": "^1.1.2",
    "prettier": "^2.7.1",
    "react-native-safe-area-context": "4.2.4",
    "react-native-screens": "~3.11.1",
    "react-native-svg": "12.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.67.6",
    "typescript": "~4.3.5",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "commitlint": "^16.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.4",
    "react-native-svg-transformer": "^1.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "echo '>> Pre-commit checks' && lint-staged && yarn prettier"
    }
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
